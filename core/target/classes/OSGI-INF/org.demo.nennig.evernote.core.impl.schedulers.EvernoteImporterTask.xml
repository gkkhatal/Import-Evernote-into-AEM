<?xml version="1.0" encoding="UTF-8"?><components xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0">
    <scr:component immediate="true" name="org.demo.nennig.evernote.core.impl.schedulers.EvernoteImporterTask" activate="activate">
        <implementation class="org.demo.nennig.evernote.core.impl.schedulers.EvernoteImporterTask"/>
        <service servicefactory="false">
            <provide interface="java.lang.Runnable"/>
        </service>
        <property name="scheduler.expression" value="*/10 * * * * ?"/>
        <property name="scheduler.concurrent" type="Boolean" value="false"/>
        <property name="evernote.username"/>
        <property name="evernote.password"/>
        <property name="evernote.dev" type="Boolean" value="false"/>
        <property name="evernote.token"/>
        <property name="service.vendor" value="Adobe"/>
        <property name="service.pid" value="org.demo.nennig.evernote.core.impl.schedulers.EvernoteImporterTask"/>
        <reference name="repository" interface="org.apache.sling.jcr.api.SlingRepository" cardinality="1..1" policy="static" bind="bindRepository" unbind="unbindRepository"/>
    </scr:component>
</components>
